[Unit]
Description=MPT Broker Inspector Daemon
After=syslog.target network.target

[Service]
Type=forking
EnvironmentFile=-/@CMAKE_INSTALL_SYSCONFIG_PATH@/mpt-inspector-daemon
ExecStart=@CMAKE_INSTALL_PREFIX@/bin/mpt-inspector-daemon -m $MAESTRO_BROKER_URL -L $MPT_LOG_DIR -l $MPT_LOG_LEVEL
PrivateTmp=false

[Install]
WantedBy=multi-user.target