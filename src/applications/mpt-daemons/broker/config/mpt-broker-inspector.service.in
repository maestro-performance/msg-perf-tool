[Unit]
Description=MPT Broker Inspector Daemon
After=syslog.target network.target

[Service]
Type=forking
EnvironmentFile=-/@CMAKE_INSTALL_SYSCONFIG_PATH@/mpt-broker-inspector
ExecStart=@CMAKE_INSTALL_PREFIX@/bin/mpt-broker-inspector -m $MAESTRO_BROKER_URL -L $MPT_LOG_DIR -l $MPT_LOG_LEVEL
PrivateTmp=false

[Install]
WantedBy=multi-user.target